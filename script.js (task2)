for (let i = 1; i <= 5; i++) //1 Цикл for
{ 
    console.log(i);
}

let i = 1; //1 Цикл while
while (i <= 5) 
{
    console.log(i);
    i++;
}

const str = "Hello"; //2 Цикл for
for (let i = 0; i < str.length; i++) 
{
    console.log(str[i]);
}

const str = "Hello";  //2 Цикл while
let i = 0;
while (i < str.length) 
{
    console.log(str[i]);
    i++;
}
